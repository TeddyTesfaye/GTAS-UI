{
  "name": "gtas-ui",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js, jsx}",
      "!<rootDir>/node_modules/",
      "!**/coverage/**",
      "!**/serviceWorker.js",
      "!**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "coverageReporters": [
      "text"
    ]
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "cross-env": "^6.0.3",
    "dotenv-webpack": "^1.8.0",
    "font-awesome": "^4.7.0",
    "i18next": "^19.0.3",
    "i18next-browser-languagedetector": "^4.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "js-cookie": "^2.2.1",
    "match-sorter": "^4.0.2",
    "node-sass": "^4.14.1",
    "pretty-quick": "^2.0.1",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-datepicker": "^2.10.1",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.7",
    "react-idle-timer": "^4.2.12",
    "react-multi-select-component": "^2.0.14",
    "react-query": "0.3.20",
    "react-querybuilder": "^3.2.0",
    "react-responsive-tabs": "^3.3.0",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "react-table": "^7.0.0-rc.15",
    "vaquita": "^1.0.9"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "rsg": "styleguidist server",
    "rsg:build": "styleguidist build",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "msw": "^0.20.5",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-styleguidist": "^11.0.5",
    "source-map-explorer": "^2.1.2"
  }
}
